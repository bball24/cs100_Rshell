echo test1 && ls -a; echo test1
ls -a || echo test2
echo test3; echo test 3; echo test3
echo test4 && echo test 4 && echo test4
echo test5 || echo test 5 || echo test5
cat fail || echo test 6 && echo test6
echo test 7 && pwd /fail/ || echo test7; echo test 7
cat fail && echo test 8; echo test8
exit
